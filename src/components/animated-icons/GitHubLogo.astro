<style>
  svg#github-animated-logo {
    background-color: inherit;
  }

  svg#github-animated-logo:hover
    :is(path#github-logo-right-ear, path#github-logo-left-ear) {
    animation-play-state: running;
  }

  path#github-logo-right-ear {
    animation: 0.7s ease-out infinite alternate;

    animation-name: shakeRightEar;
    animation-play-state: paused;
    scale: 1.01;
  }

  path#github-logo-left-ear {
    animation: 0.7s ease-out infinite alternate;

    animation-name: shakeLeftEar;
    animation-play-state: paused;
    scale: 1.01;
  }

  @keyframes shakeRightEar {
    from {
      transform: rotate(-8deg) translateY(1px);
    }

    to {
      transform: rotate(8deg);
    }
  }

  @keyframes shakeLeftEar {
    from {
      transform: rotate(8deg) translateY(1px);
    }

    to {
      transform: rotate(-8deg);
    }
  }
</style>
